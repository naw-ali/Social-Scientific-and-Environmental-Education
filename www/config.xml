<?xml version='1.0' encoding='utf-8'?>
<widget id="com.dkt.evs" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Social, Scientific and Environmental Education</name>
    <description>
        An educational geograohy-related app.
    </description>
    <author email="nawfal.nauzeer3@gmail.com" href="http://phonegap.com">
        Nauzeer Mohammad Nawfal Ali
    </author>
    <content src="index.html" />
  <access origin="*" />  <plugin name="cordova-plugin-whitelist" version="1" />

<allow-intent href="http://*/*" />
<allow-intent href="https://*/*" />
<allow-intent href="tel:*" />
<allow-intent href="sms:*" />
<allow-intent href="mailto:*" />
<allow-intent href="geo:*" />
<platform name="android">
    <allow-intent href="market:*" />
              <icon src="images/SSEE.png" density="ldpi" />
              <icon src="images/SSEE.png" density="mdpi" />
              <icon src="images/SSEE.png" density="hdpi" />
              <icon src="images/SSEE.png" density="xhdpi" />
</platform>
<platform name="ios">
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
</platform>
</widget>
